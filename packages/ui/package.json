{
  "name": "@decentraguild/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme/index.js",
      "require": "./dist/theme/index.js"
    },
    "./theme/vars.css": "./src/theme/vars.css",
    "./components": "./src/components/index.ts"
  },
  "scripts": {
    "build": "tsup src/index.ts src/theme/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/theme/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src"
  },
  "dependencies": {
    "@decentraguild/core": "workspace:*",
    "@decentraguild/display": "workspace:^",
    "@iconify/vue": "^4.1.2",
    "pinia": "^3.0.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.0.0"
  }
}
